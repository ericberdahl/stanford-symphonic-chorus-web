<!doctype html>
<html lang="en">
<head>
<meta charset="UTF-8">
{{#block "head-title"}}
	<title>Stanford Symphonic Chorus</title>
{{/block}}
<!-- Base stylesheet which imports typography, resets, header/footer layout, and other miscellaneous styles -->
<link rel="stylesheet" type="text/css" href="/css/base.css">
<!-- Page specific stylesheet for middle layout section (sidebars and content_main) -->
{{#each this.stylesheets as |css| }}
	<link rel="stylesheet" type="text/css" href="/css/{{css}}.css">
{{/each}}
{{#each this.scripts as |js| }}
	<script src="/js/{{js}}.js" type="text/javascript"></script>
{{/each}}
<script src="/js/mootools.js" type="text/javascript"></script>
<script src="/js/mediabox.js" type="text/javascript"></script>
<script src="/js/functions.js" type="text/javascript"></script>
<script src="/js/emus.js" type="text/javascript"></script>
<!--[if IE 6]>
	<style type="text/css">
		@import url(css/ie/ie_global.css);
		{{#each this.ie6_stylesheets as |css| }}
			@import url(css/ie/ie_{{css}}.css);
		{{/each}}
	</style>
<![endif]-->
<!--[if IE 7]>
	<style type="text/css">
		@import url(css/ie/ie7.css);
	</style>
<![endif]-->
</head>
<body>
<!-- TemplateBeginEditable name="top" -->
<!-- TemplateEndEditable -->
<div class="skip">
	<a href="#content_main">Skip to Content</a>
</div>
<div id="container">
	<div id="container_header">
		<div id="header">
			{{> (lookup . 'logo_include') }}
			{{> container_search }}
		</div>
	</div>
	{{> nav_topic }}
	<div id="title">
		<h1>
			{{#block "body-title"}}
				<!-- TemplateBeginEditable name="Page Title" -->
				{{ this.title }}
				<!-- TemplateEndEditable -->
			{{/block}}
		</h1>
	</div>

	{{#if (content "sidebar-intro") }}
		<div id="sidebar_introduction">
			{{{ block "sidebar-intro" }}}
		</div>
	{{/if}}

	{{#block "main-content"}}{{/block}}

	{{#if (content "sidebar-supplement") }}
		<div id="sidebar_supplementary">
			{{{ block "sidebar-supplement" }}}
			<div id="seal"></div>
		</div>
	{{/if}}
	
	{{#if (content "sidebar") }}
		<div id="sidebar">
			{{{ block "sidebar" }}}
			<div id="seal"></div>
		</div>
	{{/if}}

	{{> footer }}
</div>
<!-- TemplateBeginEditable name="bottom" -->
<!-- TemplateEndEditable -->
</body>
</html>
